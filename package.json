{
  "name": "job-portal",
  "version": "1.0.0",
  "description": "A comprehensive Job Portal Management System",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "npm install && cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "postinstall": "cd client && npm install"
  },
  "keywords": ["job-portal", "react", "nodejs", "express"],
  "author": "Job Portal Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
} 